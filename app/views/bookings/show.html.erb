<h1>Bookings#show</h1>
<p>Find me in app/views/bookings/show.html.erb</p>
<div class="container">

  <div class="row">
    <div class="col-xs-12 col-md-6 col-lg-4 col-md-offset-3 col-lg-offset-4">
      <h2>Congrats, you booked:</h2>
      <%= @booking.garden.title %>
      <h4>at: </h4>
      <%= @booking.price %>
      <h5>From: </h5>
      <%= @booking.start_date %>
      <h5>To: </h5>
      <%= @booking.end_date %>
      <h5>at: </h5>
      <%= @booking.garden.user.email %>
      <h5><%= @booking.garden.user.email %>'s place!</h5>
    </div>
  </div>
</div>
